{
  "name": "hararec-portfolio",
  "version": "1.0.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "yarn ng serve -o --port 8080",
    "dev:docker": "yarn ng serve --port 8080",
    "build:prod": "yarn ng build --configuration production --base-href https://hararecmg.github.io/hararec-website/ --aot --build-optimizer --optimization --output-path docs/",
    "build:nginx": "yarn ng build --configuration production --base-href / --aot --build-optimizer --optimization --output-path docs/",
    "build:docker": "docker compose -f docker-compose.prod.yml up",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.1",
    "@angular/cdk": "^15.1.2",
    "@angular/common": "^15.2.1",
    "@angular/compiler": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@angular/forms": "^15.2.1",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "gsap": "^3.11.4",
    "primeicons": "^6.0.1",
    "primeng": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@ngx-env/builder": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.14.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  },
  "repository": "https://github.com/hararecmg/hararec-website.git",
